<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="type" namespace="/type" extends="all-interceptors">
		<action name="*" class="com.xuri.system.action.TypeAction" method="{1}">
			<result name="show">/WEB-INF/jsp/system/typeList.jsp</result>
			<result name="typeList" type="json">
				<param name="root">typeList</param>
			</result>
		</action>
	</package>
</struts>